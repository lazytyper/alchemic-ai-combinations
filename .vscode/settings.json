{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".*\\.txt$", // Falls du nur bestimmte Dateien mÃ¶chtest, anpassen
        "cmd": "sed -i -e 's/+/_/g' ${file}"
      },
      {
        "match": "dict\\.txt|keywords\\.txt",
        "cmd": "sort ${file} | grep -v '^$' > ${file}.tmp && mv ${file}.tmp ${file}"
      }
    ]
  },
  "search.exclude": {
    "**/node_modules": true,
    "index.html": true
  }
}
